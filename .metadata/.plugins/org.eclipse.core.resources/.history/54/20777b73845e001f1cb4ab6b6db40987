package entidades;

public class Conta {
	private int numero;
	private String titular;
	private double saldo;
	
	// Construtor
	public Conta(int numero, String titular) {
		this.numero = numero;
		this.titular = titular;
	}
	
	// Sobrecarga
	public Conta(int numero, String titular, double saldo) {
		this.numero = numero;
		this.titular = titular;
		this.saldo = saldo;
	}

	
	
}
